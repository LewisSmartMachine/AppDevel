<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.loggerapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_AWS"
        android:name="com.example.loggerapp.ui.aws.AWSFragment"
        android:label="@string/title_AWS"
        tools:layout="@layout/fragment_aws" />

    <fragment
        android:id="@+id/navigation_Bluetooth"
        android:name="com.example.loggerapp.ui.Bluetooth.BluetoothFragment"
        android:label="@string/title_Bluetooth"
        tools:layout="@layout/fragment_bluetooth" />
